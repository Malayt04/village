{
  "files": [],
  "compilerOptions": {
    
    "composite": true,
    "target": "es5", // or another target like "es6"
    "module": "CommonJS", // or another module system like "esnext"
    "jsx": "react", // or "react-jsx" for React 17+
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": ["src"]
}


